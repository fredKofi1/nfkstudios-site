    // DockerRun extension setting - Disables loading of .dockerrc file
    "DockerRun.DisableDockerrc": true,

    // "DockerRun.Containers": [ ... ]
    // Removed this setting as container IDs are specific to local environments
    // and should not typically be shared in project settings.

    // --- Python Testing Settings ---
    // Note: Remove these settings if Python testing is not part of this project.
    "python.testing.unittestArgs": [
        "-v", // Verbose output
        "-s", // Directory to start discovery (current directory)
        ".",
        "-p", // Pattern for test files
        "*test.py"
    ],
    "python.testing.pytestEnabled": false, // Explicitly disable Pytest
    "python.testing.unittestEnabled": true,
    "DockerRun.Containers": [
        "32a5f7fa648f"
    ] // Explicitly enable Unittest
